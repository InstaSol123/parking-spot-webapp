{
  "name": "parking-spot-backend",
  "version": "1.0.0",
  "description": "Backend API for Parking Spot application",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "cleanup:data": "node --loader ts-node/esm cleanup-data.ts",
    "reset:credits": "npx tsc reset-credits.ts --esModuleInterop --skipLibCheck --module es2020 --target es2020 && node reset-credits.js && rm reset-credits.js",
    "cleanup:all": "npx tsc cleanup-all-data.ts --esModuleInterop --skipLibCheck --module es2020 --target es2020 && node cleanup-all-data.js && rm cleanup-all-data.js"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "sanitize-html": "^2.17.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.2",
    "jest": "^29.7.0",
    "prisma": "^5.8.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
